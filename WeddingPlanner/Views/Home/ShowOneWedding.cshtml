@{
    ViewData["Title"] = "One Wedding";
}
<div class="d-flex justify-content-between">
    <h1>Welcome!</h1>
    <p><a href="/dashboard">Home</a>| <a href="/wedding/new">Plan a Wedding</a> |
    <a asp-action="UserLogout" asp-controller="User" class="btn btn-danger">LogOut</a></p>
</div>
<div class="info">
    <h2>@ViewBag.thisWedding.WedderOne & @ViewBag.thisWedding.WedderTwo wedding!</h2>
    <h3>Date: @ViewBag.thisWedding.WeddDate.ToString("MMM d, yyyy")</h3>
    <h4>Guests:</h4>
    <ul>
        @foreach (var i in ViewBag.AllGuests)
        {
            <li>@i.User.FirstName @i.User.LastName</li>
        }
    </ul>
</div>